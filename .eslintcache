false);\n    });\n\n  }\n\n  useEffect(() => {\n    // setLoading(true);\n    // setSearch();\n    // setData();\n    // setLoading(false);\n  }, [])\n\n  return (\n\n    <>\n      <Switch>\n        \n        <Route\n          exact\n          path={'/'}\n          component={() => <Home setSearch={setSearch} />}\n        />\n  {/* {!NewsArticles ? <div className=\"search-page\">\n              <HeaderSearchPage searchTerm={searchTerm} setSearch={setSearch} />\n              <h4 style={{ fontFamily: 'cursive', textAlign: 'center', position: 'relative', top: '70px' }}>\n                Loading....\n              </h4>\n            </div> : \"\"} */}\n          \n        {searchTerm !== '' && !Loading ? (\n          \n            <Route\n              exact\n              path={'/search'}\n              component={() => (\n\n                <NewsCards\n                  articles={NewsArticles}\n                  activeArticle={activeArticles}\n                  searchTerm={searchTerm}\n                  setSearch={setSearch}\n                  loading={Loading}\n                />\n              )}\n            />\n        \n        ) :\n          <div className=\"search-page\">\n            <HeaderSearchPage searchTerm={searchTerm} setSearch={setSearch} />\n            <h4 style={{ fontFamily: 'cursive', textAlign: 'center', position: 'relative', top: '70px' }}>\n              Loading....\n            </h4>\n          </div>}\n\n\n\n        <Redirect to=\"/\" />\n      </Switch>\n    </>\n  );\n};\n\nexport default App;\n","F:\\Programming code\\react code\\NewsSearch Insight Engine\\NewsSearch Insight Engine\\src\\components\\LoadingSpinner\\LoadingSpinner.js",[],"F:\\Programming code\\react code\\NewsSearch Insight Engine\\NewsSearch Insight Engine\\src\\components\\Search\\HeaderSearchPage.js",[],"F:\\Programming code\\react code\\NewsSearch Insight Engine\\NewsSearch Insight Engine\\src\\components\\NewsCards\\NewsCards.js",["151","152","153","154"],"import React